{
  "name": "donus-test",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Lázaro Simão",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/server.js",
    "start:dev": "ts-node-dev 'src/server.ts'",
    "test": "NODE_ENV=test jest",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src __tests__ --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "typeorm": "TS_NODE_PROJECT=tsconfig.ts-node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js",
    "migrate:run": "yarn typeorm migration:run",
    "migrate:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.25",
    "typeorm-transactional-cls-hooked": "^0.1.12"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.1",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.6.0",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.59",
    "typescript": "^4.0.2"
  }
}
